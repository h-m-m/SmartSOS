<% if session[:organization_id] || session[:donor_id] %>
	<%= link_to "Register", new_donor_path %>
	<%= link_to "Register As Organization", new_organization_path %>
	<%= link_to "Log In", donors_login_path %>
	<%= link_to "Organization Log In", organizations_login_path %>
<% else %>
	<%= link_to "Log Out", sessions_logout_path %>
<% end %>
